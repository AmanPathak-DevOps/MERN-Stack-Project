;\n    const requestOptions = {\n      method: 'POST',\n      headers: { 'Content-Type': 'application/json' },\n      body: JSON.stringify( studentData )\n  };\n    \n    fetch(`${API_BASE_URL}/addstudent`, requestOptions ).then((res) => res.json())\n.then(() => {\ngetData()})\n      .catch((err) => console.log(err));\n  };\n  const handleDelete = (id) => {\n    fetch(`${API_BASE_URL}/student/${id}`, {\n      method: 'DELETE',\n    })\n      .then((res) => res.json())\n      .then(() => {\n        getData();\n      })\n      .catch((err) => console.error('Error deleting data:', err));\n  };\n\n  return (\n    <div>\n    <div className=\"student-container\">\n      <div className=\"content\">\n        <h2 className='store-student-details'style={{ marginLeft: '100px' }}>Student Details</h2>\n        <form onSubmit={handleSubmit}>\n          <div className=\"form-group\">\n            <label>Name:</label>\n            <input\n              type=\"text\"\n              name=\"name\"\n              value={studentData.name}\n              onChange={handleInputChange}\n              required\n            />\n          </div>\n          <div className=\"form-group\">\n            <label>Roll No (Must be Number):</label>\n            <input\n              type=\"text\"\n              name=\"rollNo\"\n              value={studentData.rollNo}\n              onChange={handleInputChange}\n              required\n            />\n          </div>\n          <div className=\"form-group\">\n            <label>Class:</label>\n            <input\n              type=\"text\"\n              name=\"class\"\n              value={studentData.class}\n              onChange={handleInputChange}\n            />\n          </div>\n          <div className=\"form-group\">\n          <p style={{ marginLeft: '50px' }}>\n            <button type=\"submit\">Submit</button>\n            </p>\n          </div>\n        </form>\n      </div>\n      <div style={{ display: 'flex', justifyContent: 'flex-e